{
  "devDependencies": {
    "cypress": "^13.15.2",
    "playwright-webkit": "^1.49.0",
    "mochawesome": "^7.1.3"

  },
  "scripts": {
    "test:chrome": "cypress run --browser chrome --reporter mochawesome",
    "test:firefox": "cypress run --browser firefox --reporter mochawesome",
    "test:edge": "cypress run --browser edge --reporter mochawesome",
    "test:webkit": "cypress run --browser webkit --reporter mochawesome",
    "test": "npm run test:chrome && npm run test:firefox && npm run test:edge && npm run test:webkit"
  }
}
